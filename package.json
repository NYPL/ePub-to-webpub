{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "start:run": "tsdx watch --onSuccess 'node dist/index.js'",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "epub-to-webpub",
  "author": "kristojorg",
  "module": "dist/epub-to-webpub.esm.js",
  "size-limit": [
    {
      "path": "dist/epub-to-webpub.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/epub-to-webpub.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/xml2js": "^0.4.8",
    "husky": "^6.0.0",
    "msw": "^0.28.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/xmldom": "^0.1.30",
    "node-fetch": "^2.6.1",
    "r2-shared-js": "^1.0.50",
    "r2-utils-js": "^1.0.24",
    "xml2js": "^0.4.23",
    "xmldom": "^0.5.0"
  }
}
